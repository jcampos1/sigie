{% extends '::base2.html.twig' %}
{% block title %}Ver-Subtema{% endblock %}
{% block body -%}
    {% for flashMessage in app.session.flashbag.get('mensaje') %}
        <div class="alert alert-success text-center">
            {{ flashMessage }}
        </div>
    {% endfor %}
    <h1><small>SUBTEMA</small></h1>

    <table class="record_properties table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Unidad</th>
                <td>{{ entity.unidad }}</td>
            </tr>
        </tbody>
    </table>

    {% set listado = "subtema" %}
    {% set editar = "subtema_edit" %}
    <div id="control">
    {{ include("::regre_edit_elim.html.twig") }}
    </div>

{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/funciones.js') }}"></script>
{% endblock javascripts %}
